#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules/@sveltejs/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules/@sveltejs/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/@sveltejs+kit@2.16.0_@svelt_9d79c0d80fccda533d4723c5b82f2397/node_modules:/mnt/c/Users/USER 1/Documents/my-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
